{
    "name": "Eson",
    "clusterConfigurationVersion": "1.0.0",
    "apiVersion": "10-2017",
    "nodes": [
        {
            "nodeName": "SF1",
            "iPAddress": "SF1",
            "nodeTypeRef": "NodeType0",
            "faultDomain": "fd:/dc1/r0",
            "upgradeDomain": "UD0"
        },
        {
            "nodeName": "SF2",
            "iPAddress": "SF2",
            "nodeTypeRef": "NodeType0",
            "faultDomain": "fd:/dc1/r1",
            "upgradeDomain": "UD1"
        },
        {
            "nodeName": "SFMaster",
            "iPAddress": "sfmaster",
            "nodeTypeRef": "NodeType0",
            "faultDomain": "fd:/dc1/r2",
            "upgradeDomain": "UD2"
        }
    ],
    "properties": {
        "diagnosticsStore": 
        {
            "metadata":  "Diagnostics data on NAS",
            "dataDeletionAgeInDays": "21",
            "storeType": "FileShare",
            "connectionstring": "\\\\nas\\Save\\servicefabric"
        },
        "security": {
            "ClusterCredentialType": "Windows",
            "ServerCredentialType": "Windows",
            "WindowsIdentities": {
                "ClustergMSAIdentity": "YX\\gmsa-sf",
                "ClusterSPN": "ServiceFabric/gmsa-sf/yx",
                "ClientIdentities": [
                    {
                        "Identity": "YX\\SFAdmins",
                        "IsAdmin": true
                    }
                ]
            }
        },
        "nodeTypes": [
            {
                "name": "NodeType0",
                "clientConnectionEndpointPort": "19000",
                "clusterConnectionEndpointPort": "19001",
                "leaseDriverEndpointPort": "19002",
                "serviceConnectionEndpointPort": "19003",
                "httpGatewayEndpointPort": "19080",
                "reverseProxyEndpointPort": "19081",
                "applicationPorts": {
                    "startPort": "20001",
                    "endPort": "20031"
                },
                "ephemeralPorts": {
                    "startPort": "49152",
                    "endPort": "65535"
                },
                "isPrimary": true
            }
        ],
        "fabricSettings": [
            {
                "name": "Setup",
                "parameters": [
                    {
                        "name": "FabricDataRoot",
                        "value": "C:\\SF"
                    },
                    {
                        "name": "FabricLogRoot",
                        "value": "C:\\SF\\Log"
                    }
                ]
            }
        ]
    }
}